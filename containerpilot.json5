{
	consul: "localhost:8500",
	logging: {
		level: "INFO",
		format: "default",
		output: "stdout"
	},
	jobs: [
		{
			name: "consul",
			exec: "/usr/local/bin/start_consul",
			restarts: "unlimited",
			health: {
				exec: "consul-healthcheck",
				interval: 20,
				ttl: 60,
				timeout: "5s"
			}
		},
		{
			name: "vault",
			exec: "/usr/local/bin/start_vault",
			restarts: "never",
			when: {
				source: "consul",
				once: "healthy"
			},
		}
	]
}
